# Telemetry Glue Environment Variables
# Copy this file to .env and set your actual values
# Set ENV to "production" in your environment variables (NOT .env) to disable .env file loading

# New Relic API configuration
NEW_RELIC_API_KEY=your_api_key_here
NEW_RELIC_ACCOUNT_ID=your_account_id_here

# OpenTelemetry configuration (for otel-test-sender)
NEW_RELIC_INGEST_API_KEY=your_ingest_api_key_here
NEW_RELIC_OTLP_ENDPOINT=otlp.nr-data.net:4318
